	"version": "0.2.0",
	"configurations": [{
		"cwd": "${workspaceFolder}",
		"name": "Serve",
		"outFiles": [
			"${workspaceFolder}/dist/**.js"
		],
		"program": "${workspaceFolder}/source/server/server.ts",
		"request": "launch",
		"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
		"runtimeArgs": [
			// "--nolazy",
			"--config-file", "./babel.config.js",
			"--extensions", ".ts,.tsx,.js",
		],
		"type": "node"
	}]
}
